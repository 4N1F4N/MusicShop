<form action = "<%= admin_new_product_path %>" method = 'post'>
  <h5>Создать продукт</h5>
  <input type="text" name="product[title]" placeholder="Наименование">
  <input type="number" name="product[price]" placeholder="Цена">
  <select class="form-select" name="product[tag_id]">
    <% Tag.all.each do |tag|%>
      <option value="<%= tag.id %>"><%= tag.title %></option>
    <% end %>
  </select>
  <input type="submit" name="submit" value="Создать продукт">
</form>